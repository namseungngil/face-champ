          <boolProp name="resetInterpreter">false</boolProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="script">import java.util.Random;
import java.util.UUID;

Random rand = new Random();
int threadNum=ctx.getThreadNum();

int type = rand.nextInt(3);
String identifier = UUID.randomUUID().toString();

vars.put(&quot;type&quot;, Integer.toString(type));
vars.put(&quot;identifier&quot;, identifier);

boolean samplerHeader = &quot;true&quot;.equalsIgnoreCase(vars.get(&quot;sampler.header&quot;));
if (samplerHeader) {
	vars.put(&quot;header&quot;, threadNum+&quot; : &quot;);
}else{
	vars.put(&quot;header&quot;, &quot;&quot;);
}

String accountName = &quot;account name #&quot; + threadNum + &quot; @&quot; + System.currentTimeMillis();
String accountBio = &quot;bio @&quot; + System.currentTimeMillis();

vars.put(&quot;accountName&quot;, accountName);
vars.put(&quot;accountBio&quot;, accountBio);</stringProp>
        </BeanShellPreProcessor>
